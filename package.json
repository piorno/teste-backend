{
  "name": "teste-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --files -r tsconfig-paths/register --inspect --ignore-watch node_modules src/index.ts",
    "migration:generate": "npx typeorm-ts-node-esm migration:generate ./src/migrations/new_migration -d ./src/data-source.ts",
    "migration:run": "npx typeorm-ts-node-esm migration:run -d src/data-source.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.17.3",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.9.5"
  }
}
